{
  "name": "frontend-iac",
  "version": "0.1.0",
  "bin": {
    "frontend-iac": "bin/frontend-iac.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy:dev": "cdk deploy --all -c stage=dev --outputs-file ./frontend-dev.json --require-approval never",
    "diff:dev": "cdk diff -c stage=dev",
    "destroy:dev": "cdk destroy --all -c stage=dev --require-approval never"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.173.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.173.4",
    "cdk-nag": "^2.34.23",
    "constructs": "^10.0.0"
  }
}
